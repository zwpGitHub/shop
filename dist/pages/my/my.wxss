Page {
  background-color: #f2f8ff;
}
.indexBox {
  box-sizing: border-box;
  width: 100%;
}
.myTop {
  box-sizing: border-box;
  width: 100%;
  height: 170rpx;
  background: #F83760;
}
.myBody {
  box-sizing: border-box;
  margin-top: -90rpx;
  padding: 0 30rpx;
  height: 100rpx;
}
.myMsgBox {
  width: 690rpx;
  height: 180rpx;
  background: #ffffff;
  box-shadow: 0px 3px 29px 0px rgba(248, 55, 96, 0.18);
  border-radius: 16px;
  position: relative;
  box-sizing: border-box;
  padding-top: 100rpx;
  margin-bottom: 26rpx;
}
.headerImg {
  width: 130rpx;
  height: 130rpx;
  background: #ffffff;
  box-shadow: 0px 1px 29px 0px rgba(248, 55, 96, 0.21);
  border-radius: 50%;
  position: absolute;
  left: 50%;
  margin-left: -65rpx;
  top: -65rpx;
}
.myName {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #5B5B5B;
}
.myAddress {
  width: 100%;
  height: 109rpx;
  background: #ffffff;
  border-radius: 16rpx;
  box-sizing: border-box;
  padding: 0 27rpx;
  display: flex;
  align-items: center;
  margin-bottom: 13rpx;
}
.myImg {
  width: 40rpx;
  height: 40rpx;
  margin-right: 25rpx;
}
.myText {
  flex: 1;
  font-size: 30rpx;
  color: #3F3F3F;
}
.myIcon {
  width: 12rpx;
  height: 22rpx;
}
.myOrderBox {
  width: 100%;
  background: #ffffff;
  border-radius: 16rpx;
  margin-bottom: 13rpx;
  box-sizing: border-box;
  padding: 0 28rpx;
}
.allOrder {
  font-size: 24rpx;
  color: #A9A9A9;
  display: flex;
  align-items: center;
}
.myOrderBoxTop {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eeeeee;
}
.myOrderBoxBot {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 0;
  justify-content: space-between;
}
.myOrderItem {
  display: flex;
  align-items: center;
  flex-direction: column;
  color: #A9A9A9;
  font-size: 24rpx;
}
.orderImg {
  width: 50rpx;
  height: 42rpx;
  margin-bottom: 20rpx;
}
