Page {
  background-color: #f2f8ff;
}
.indexBox {
  box-sizing: border-box;
  width: 100%;
  padding-bottom: 110rpx;
}
.addAddress {
  width: 100%;
  height: 90rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 90rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: linear-gradient(77deg, #fd7b35, #fd4254);
  color: #fff;
  font-size: 28rpx;
}
.msgBox {
  box-sizing: border-box;
  width: 100%;
  padding: 0 30rpx;
  background-color: #fff;
  margin-bottom: 11rpx;
}
.default {
  box-sizing: border-box;
  height: 110rpx;
  width: 100%;
  background: #fff;
  font-size: 24rpx;
  color: #5C5D5D;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
}
.msgInput {
  background: none;
  outline: none;
  border: 0px;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #CDCDCD;
  color: #353535;
  font-size: 24rpx;
}
.areaBox {
  color: #BDBDBD;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-bottom: 1rpx solid #CDCDCD;
  padding: 40rpx 0;
}
.area {
  flex: 1;
}
.areaChoose {
  flex: 1;
  color: #353535;
}
.myIcon {
  width: 12rpx;
  height: 22rpx;
}
.deleteAdd {
  font-size: 24rpx;
  color: #FB4A5E;
  width: 100%;
  margin-top: 13rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  height: 110rpx;
  line-height: 110rpx;
  background: #fff;
}
