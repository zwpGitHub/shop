Page {
  background-color: #f2f8ff;
}
.indexBox {
  box-sizing: border-box;
  width: 100%;
  padding-bottom: 110rpx;
}
.addressList {
  width: 100%;
  background-color: #fff;
  margin-bottom: 10rpx;
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.addImg {
  width: 28rpx;
  height: 33rpx;
  margin-right: 13rpx;
}
.editAddress {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}
.edit {
  font-size: 24rpx;
  color: #8D8D8D;
  margin-left: 25rpx;
}
.eidtLine {
  width: 2rpx;
  height: 45rpx;
  background: #cfcfcf;
}
.msgBox {
  flex: 1;
}
.msgBoxTop {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}
.msgBoxBot {
  color: #303030;
  font-size: 24rpx;
  line-height: 42rpx;
}
.name {
  color: #313131;
  font-size: 24rpx;
  max-width: 200rpx;
}
.phone {
  font-size: 24rpx;
  color: #B8B8B8;
  margin-left: 40rpx;
}
.default {
  display: inline-block;
  margin-right: 10rpx;
  width: 76rpx;
  height: 36rpx;
  background: #fbacac;
  border-radius: 4rpx;
  text-align: center;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #fff;
}
.addAddress {
  width: 100%;
  height: 90rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 90rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: linear-gradient(77deg, #fd7b35, #fd4254);
  color: #fff;
  font-size: 28rpx;
}
