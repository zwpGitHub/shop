Page {
  background-color: #f2f8ff;
}
.indexBox {
  box-sizing: border-box;
  width: 100%;
}
.addBox {
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}
.addLine {
  height: 10rpx;
  width: 100%;
  margin-bottom: 10rpx;
}
.addressImg {
  height: 27rpx;
  width: 23rpx;
  margin-right: 13rpx;
}
.addIcon {
  width: 12rpx;
  height: 22rpx;
  margin-left: 22rpx;
}
.msgBox {
  flex: 1;
  font-size: 24rpx;
  color: #5B5B5B;
}
.name {
  margin-bottom: 15rpx;
}
.address {
  line-height: 42rpx;
}
.goodsBox {
  box-sizing: border-box;
  padding-bottom: 98rpx;
}
.goodsList {
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #fff;
  margin-bottom: 10rpx;
  display: flex;
}
.goodsImg {
  margin-right: 33rpx;
  height: 141rpx;
  width: 141rpx;
}
.goodsMsg {
  flex: 1;
  height: 141rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.goodsTitle {
  font-size: 20rpx;
  color: #3F3F3F;
}
.goodsPrice {
  display: flex;
  justify-content: space-between;
}
.price {
  color: #F83760;
  font-size: 30rpx;
}
.num {
  font-size: 24rpx;
  color: #A9A9A9;
}
.allNum {
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #B6B6B6;
  width: 100%;
  text-align: right;
  margin-top: 20rpx;
}
.botBtn {
  height: 98rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.allPrice {
  font-size: 24rpx;
  color: #474747;
  margin-right: 10rpx;
}
.priceNum {
  font-size: 36rpx;
  color: #FD4254;
  margin-right: 30rpx;
}
.btn {
  width: 200rpx;
  height: 62rpx;
  background: linear-gradient(55deg, #fd7b35, #fd4254);
  border-radius: 31rpx;
  text-align: center;
  line-height: 62rpx;
  font-size: 24rpx;
  color: #fff;
}
.bgBox {
  box-sizing: border-box;
  position: fixed;
  z-index: 10;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}
.bg {
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}
.box {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 400rpx;
  width: 100%;
  background-color: #fff;
  border-radius: 30rpx 30rpx 0px 0px;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.payBtn {
  width: 100%;
  height: 62rpx;
  background: linear-gradient(55deg, #fd7b35, #fd4254);
  border-radius: 31rpx;
  line-height: 62rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
}
.boxTitle {
  text-align: center;
  font-size: 36rpx;
  color: #575757;
  padding: 35rpx 0;
  border-bottom: 1rpx solid #C3C3C3;
}
.payPrice {
  text-align: center;
  padding: 60rpx 0;
  color: #FD4254;
  font-size: 50rpx;
}
