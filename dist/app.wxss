page {
  height: 100%;
  background-color: #f2f8ff;
}
text {
  line-height: 1.1em;
  font-family: 'PingFang SC';
}
image {
  vertical-align: top;
}
.pre-initShow {
  opacity: 0;
  transform: translateX(50rpx);
}
.initShow-1 {
  opacity: 1;
  transform: translateX(0);
  transition: 0.3s;
}
.initShow-2 {
  opacity: 1;
  transform: translateX(0);
  transition: 0.3s 0.1s;
}
.initShow-3 {
  opacity: 1;
  transform: translateX(0);
  transition: 0.3s 0.2s;
}
.ellip-2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: normal;
}
.ellip-3 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  white-space: normal;
}
.ellip-1 {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.formIdBtn {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0;
  margin: 0;
  background-color: transparent;
}
.formIdBtn::after {
  content: none;
}
